#include <iostream>
using namespace std;

void swapNumbers(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
    cout << "Inside swap function: a = " << a << ", b = " << b << endl;
}
int main() {
    int x, y;
    cout << "Enter first number: ";
    cin >> x;
    cout << "Enter second number: ";
    cin >> y;
    cout << "Before swapping: x = " << x << ", y = " << y << endl;
    swapNumbers(x, y); 
    cout << "After calling swap function: x = " << x << ", y = " << y << endl;
    return 0;
}
